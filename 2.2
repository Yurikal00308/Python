import pulp

problem = pulp.LpProblem('SL' , pulp.LpMaximize)

x = pulp.LpVariable('x', cat='Continuous')
y = pulp.LpVariable('y', cat='Continuous')

problem += x + 3 * y <= 30
problem += 2 * x + y <= 40
problem += x >= 0
problem += y >= 0
problem += x + 2 * y

status = problem.solve()

print ('Status:', pulp.LpStatus[status])
print
